 Set new).self assert: (f5 value: Set new).self assert: (f5 value: vars1) not.self assert: (f5 value: vars2).self assert: (f5 value: vars3).! !PropositionalFormula subclass: #Disjunction	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PLPFormulaTests'!!Disjunction methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 14:41'!setForm1: unaForm form2: otraForm	formula1:= unaForm.	formula2:= otraForm.! !!Disjunction methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 23:21'!setMensaje	mensaje :='|'.! !!Disjunction methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/14/2016 09:55'!negate^(formula1 negate) & (formula2 negate). ! !!Disjunction methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 16:54'!value:variables	^(formula1 value:variables) or: (formula2  value: variables).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Disjunction class	instanceVariableNames: ''!!Disjunction class methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 23:21'!of: aForm and: otraForm	^(self  new setForm1: aForm form2: otraForm) setMensaje.! !PropositionalFormula subclass: #Implication	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'PLPFormulaTests'!!Implication methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/14/2016 10:41'!toNNF	^ (formula1 negate) | formula2. ! !!Implication methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 17:31'!setForm1: unaForm form2: otraForm	formula1:= unaForm.	formula2:= otraForm. ! !!Implication methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 23:22'!setMensaje	mensaje :='==>'.! !!Implication methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/14/2016 09:52'!negate^ formula1  & (formula2 negate). ! !!Implication methodsFor: 'as yet unclassified' stamp: 'Anonymous 6/13/2016 16:56'!value: variables				^((formula1 value: variables)not) or: (formula2 v